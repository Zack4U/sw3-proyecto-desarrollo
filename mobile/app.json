{
  "expo": {
    "name": "ComiYa",
    "slug": "comiya",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "comiya",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2e7d32"
    },

    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "androidMode": "default"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.comiya.app"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.INTERNET"],
      "package": "com.comiya.app"
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "460f1ff5-d1b7-4a4f-9b43-83442fc18a9e"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.YOUR_GOOGLE_IOS_CLIENT_ID"
        }
      ]
    ],
    "owner": "zack4u"
  }
}
